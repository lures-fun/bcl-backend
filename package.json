  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json --coverage",
    "test:owasp": "owasp-dependency-check --project \"bcl\" -f HTML JSON",
    "migration:create": "NODE_ENV=prd npx typeorm migration:create src/migration",
    "migration:generate": "NODE_ENV=prd npx typeorm-ts-node-commonjs migration:generate src/migration/updateTable -d src/data-source.ts",
    "migration:run": "NODE_ENV=prd npx typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "migration:create:dev": "NODE_ENV=dev npx typeorm migration:create src/migration",
    "migration:generate:dev": "NODE_ENV=dev npx typeorm-ts-node-commonjs migration:generate src/migration/updateTable -d src/data-source.ts",
    "migration:run:dev": "NODE_ENV=dev npx typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "migration:create:test": "NODE_ENV=test npx typeorm migration:create src/migration",
    "migration:generate:test": "NODE_ENV=test npx typeorm-ts-node-commonjs migration:generate src/migration/updateTable -d src/data-source.ts",
    "migration:run:test": "NODE_ENV=test npx typeorm-ts-node-commonjs migration:run -d src/data-source.ts"
  },
